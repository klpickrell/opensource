<script language="JavaScript1.1" type="text/javascript">
function hideLayer(elementId) {
 if(document.getElementById(elementId) != null) {
      document.getElementById(elementId).style.display = "none";
 }
}

function showLayer(elementId) {
 if(document.getElementById(elementId) != null) {
      document.getElementById(elementId).style.display = "block";
 }
}

function AdjustLayer(elementId, optionalLeftOffset, optionalTopOffset) {
  var topAdjustment = 23;
  var leftAdjustment = 0;
  leftAdjustment = (optionalLeftOffset == null || optionalLeftOffset.length == 0) ? leftAdjustment : optionalLeftOffset;
  topAdjustment = (optionalTopOffset == null || optionalTopOffset.length == 0) ? topAdjustment : optionalTopOffset;
  if(document.getElementById(elementId) != null) {
    var position;
    if(document.getElementById("checkOutCartIcon") != null){
      position = getPosition(document.getElementById("checkOutCartIcon"));
    }
    if(position != null) {
    document.getElementById(elementId).style.left = position.left + leftAdjustment + 'px';
    document.getElementById(elementId).style.top = position.top + topAdjustment + 'px';
    }
    document.getElementById(elementId).style.display = "block";
  }
}

function getPosition(element) {
  var position = null;
  if(element != null){
    position = {left:element.offsetLeft,top:element.offsetTop};
    if(element.offsetParent) {
      var parentPosition = getPosition(element.offsetParent);
      if(parentPosition != null) {
        position = {left:position.left +=  parentPosition.left,top:position.top += parentPosition.top};
      }
    }
  }
  return position;
}
</script>

