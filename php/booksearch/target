<script language="JavaScript" type='text/javascript'><!--
/* E-commerce Variables */
if(!s.campaign) s.campaign = ""; 
s.state = ""; 
s.zip = ""; 
s.purchaseID = ""; 
if(!s.events) s.events = "event7"; 
if(!s.eVar10) s.eVar10 = "Search Results"; 
if(!s.pageName) s.pageName = "Search Results"; 
